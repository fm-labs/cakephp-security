{
    "name": "fm-labs/cakephp-security",
    "description": "Security plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "suggest": {
        "paragonie/csp-builder": "Content security policy builder"
    },
    "autoload": {
        "psr-4": {
            "Security\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Security\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
